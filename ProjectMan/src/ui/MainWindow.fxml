<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<TabPane maxHeight="-Infinity" 
         maxWidth="-Infinity" 
         minHeight="-Infinity" 
         minWidth="-Infinity" 
         prefHeight="400.0" 
         prefWidth="600.0" 
         tabClosingPolicy="UNAVAILABLE" 
         xmlns="http://javafx.com/javafx/8" 
         xmlns:fx="http://javafx.com/fxml/1" 
         fx:controller="ui.MainWindowController"
>

    <tabs>
        <Tab text="Untitled Tab 1">
            <content>
                <AnchorPane minHeight="0.0" 
                            minWidth="0.0" 
                            prefHeight="180.0" 
                            prefWidth="200.0">
                    <children>
                        <TableView fx:id="timeTable" 
                                   editable="true" 
                                   layoutX="138.0" 
                                   prefHeight="361.0" 
                                   prefWidth="463.0">
                            <columns>
                                <TableColumn fx:id="nameColumn" 
                                             prefWidth="71.0" 
                                             resizable="false" 
                                             text="Vardas" />
                                <TableColumn fx:id="lastNameColumn" 
                                             prefWidth="79.0" 
                                             text="Pavarde" />
                                <TableColumn fx:id="positionColumn" 
                                             prefWidth="75.0" 
                                             text="Pareigos" />
                                <TableColumn fx:id="hourColumn" 
                                             prefWidth="75.0" 
                                             text="Valandos" />
                                <TableColumn fx:id="accessColumn" 
                                             prefWidth="130.0" 
                                             text="Prieinamumas" />
                            </columns>
                            <columnResizePolicy>
                                <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
                            </columnResizePolicy>
                        </TableView>
                        <Pane prefHeight="400.0" 
                              prefWidth="138.0">
                            <children>
                                <Button fx:id="addTeamMember" 
                                        layoutX="31.0" 
                                        layoutY="293.0" 
                                        mnemonicParsing="false" 
                                        onAction="#TeamMemberWindowInitializer" 
                                        prefHeight="29.0" 
                                        prefWidth="77.0" 
                                        text="Pridėti" />
                                <Label layoutX="41.0" 
                                       layoutY="268.0" 
                                       text="Personalas" />
                                <Button fx:id="addTeam" 
                                        layoutX="25.0" 
                                        layoutY="230.0" 
                                        mnemonicParsing="false" 
                                        onAction="#TeamSetupWindowInitializer" 
                                        text="Sukurti Nauja" />
                                <MenuButton fx:id="teamMenuButton" 
                                            layoutX="20.0" layoutY="200.0" 
                                            mnemonicParsing="false" 
                                            prefHeight="25.0" 
                                            prefWidth="97.0" 
                                            text="Komandos">
                                    <items>
                                        <MenuItem mnemonicParsing="false" 
                                                  text="Action 1" />
                                        <MenuItem mnemonicParsing="false" 
                                                  text="Action 2" />
                                    </items>
                                </MenuButton>
                                <Pane prefHeight="168.0" 
                                      prefWidth="137.0" 
                                      style="-fx-border-color: black;">
                                    <children>
                                        <Button fx:id="addNewWorker" 
                                                layoutX="23.0" 
                                                layoutY="59.0" 
                                                mnemonicParsing="false" 
                                                onAction="#AddNewWindowInitializer" 
                                                prefHeight="51.0" 
                                                prefWidth="92.0" 
                                                text="Sukurti darbuotoją" 
                                                textAlignment="CENTER" 
                                                wrapText="true" />
                                    </children>
                                </Pane>
                            </children>
                        </Pane>
                    </children>
                </AnchorPane>
            </content>
        </Tab>
    </tabs>
</TabPane>
